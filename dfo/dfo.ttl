dfo:Databank_Family a owl:Class;
  rdfs:subClassOf <http://purl.obolibrary.org/obo/NCIT_C79273>, <http://purl.obolibrary.org/obo/NCIT_C61393>;
  rdfs:isDefinedBy <https://w3id.org/dfo/DFO_0000001>;
  rdfs:label "Databank Family";
  dc:description "Here a detailed description of databank family." .

dfo:Hospital_Administrative_Records a owl:Class;
  rdfs:subClassOf dfo:Databank_Family;
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/NCIT_C15253>;
  rdfs:label "Hospital Administrative Records";
  dc:description "Hospital Administrative Records. Originator: Healthcare payer. Organizations which collect data: Healthcare service providers; hospitals. Prompts for records in the family data bank: Discharge from a hospitalization; a specialist encounter or emergency room visit may also prompt records. Typical content: Diagnosis/signs/symptoms observed; main diagnosis that led to the hospitalization; administration of procedures; specialty of the ward; outcome." .

dfo:Primary_Care_Medical_Records a owl:Class;
  rdfs:subClassOf dfo:Hospital_Administrative_Records;
  rdfs:isDefinedBy <https://w3id.org/dfo/DFO_0000002>;
  rdfs:label "Primary Care Medical Records";
  dc:description "Primary Care Medical Records. Originator: Network of primary care practices. Organizations which collect data: Primary care practices. Prompts for records in the family data bank: Contact between patients and their primary care practice: face-to-face/telephone/online. Records may be prompted when hospital discharge information is received by the practice (e.g. UK data banks). Typical content: Registration with the practice; diagnosis/signs/symptoms; prescription of diagnostic tests/medicines." .

dfo:Pharmacy_Dispensings_Records a owl:Class;
  rdfs:subClassOf dfo:Databank_Family;
  rdfs:isDefinedBy <http://purl.obolibrary.org/obo/OMIT_0011595>;
  rdfs:label "Pharmacy Dispensings Records";
  dc:description "Pharmacy Dispensings Records" .
